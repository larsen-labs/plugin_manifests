{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://raw.githubusercontent.com/larsen-labs/plugin_manifests/master/schema.json",
  "title": "插件概要",
  "type": "object",
  "properties": {
   "package": {
    "description": "包名",
    "type": "string"
   },
   "language": {
    "description": "程序包编程语言",
    "type": "string"
   },
   "author": {
    "description": "作者",
    "type": "string"
   },
   "description": {
    "description": "说明",
    "type": "string"
   },
   "package_version": {
    "description": "包版本",
    "type": "string"
   },
   "plugin_manifest_version": {
    "description": "插件清单版本",
    "type": "string"
   },
   "larsen_os_version_requirement": {
    "description": "需要的Larsen系统版本",
    "type": "string"
   },
   "plugin_tools_version_requirement": {
    "description": "需要的插件工具版本",
    "type": "string"
   },
   "url": {
    "description": "此包的URL",
    "type": "string"
   },
   "zip": {
    "description": "发布包的URL",
    "type": "string"
   },
   "executable": {
    "description": "可执行文件的名称",
    "type": "string"
   },
   "args": {
    "description": "可执行文件的参数",
    "type": "string"
   }
  },
  "required": ["package", "author", "zip", "executable", "args",
    "package_version", "larsen_os_version_requirement",
    "plugin_manifest_version_requirement", "plugin_tools_version_requirement"
  ]
 }
 